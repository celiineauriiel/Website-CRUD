{
    "name": "kelompok11/aplikasi-mahasiswa",
    "description": "Aplikasi CRUD untuk manajemen data mahasiswa.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kelompok 11",
            "email": "celineauriel@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "robmorgan/phinx": "^0.13.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phinx": "phinx"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}